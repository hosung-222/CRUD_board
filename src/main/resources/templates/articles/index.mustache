{{>layouts/header}}

<!--리스트 목록 페이지-->

<table class="table">
    <thead>
    <tr>

        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}}
        <tr>
            <th >{{id}}</th>
            <td><a href="/articles/{{id}}">{{title}}</a></td>  <!--  상세 페이지 접근 url 설정-->
            <td>{{content}}</td>

        </tr>
    {{/articleList}}
    </tbody>
</table>

<a href="/articles/new">New Article</a>

{{>layouts/footer}}